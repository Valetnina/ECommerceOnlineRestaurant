
                   {% for rating in reviewList %}
                        <div class="row" style="margin: 25px 0 0 0">
                            <div class="col-md-12">
                                {% if rating.rating  > 0 %}
                                    {% for i in range(1,rating.rating) %}
                                    <span class="glyphicon glyphicon-star"></span>
                                    {% endfor %}
                                    {% endif %}
                                    {% set t = (5 - rating.rating) %}
                                    {% if t > 0 %}
                                    {% for s in range(1,t) %}
                                    <span class="glyphicon glyphicon-star-empty" id="star-empty"></span>
                                    {% endfor %}
                                    {% endif %}

                                {{rating.customerFirstName}}
                                <span class="pull-right">{{rating.daysCount }} {{"days ago"|trans}}</span><br>
                                <p>{{rating.review}}</p>
                            </div>
                        </div>
                           {% endfor %}
                             <ul class="pagination">
                                 {% if totalPages >0 %}
                        {% set k = page-1 %}
                        {% if k > 0 %}
                        <li><button id="previous" onclick="changePage({{k}})">«</button></li>
                        {% endif %}
                            
                            <li><button onclick="changePage({{page}})" id="pageButton" class="pageButton">{{page}}</button></li>
                                                
                                                  

                            {% set s = page+1 %}
                              {% if page < totalPages %}
                            <li><button id="next" onclick="changePage({{s}})">»</button></li>
                            {% endif %}
                              {% endif %}
                    </ul>

